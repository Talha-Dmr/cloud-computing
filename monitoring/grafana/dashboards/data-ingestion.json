        "showThresholdMarkers": true
      },
      "pluginVersion": "8.0.0",
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"data-ingestion\",status!=\"200\"}[5m]) / rate(http_requests_total{job=\"data-ingestion\"}[5m]) * 100",
          "refId": "A"
        }
      ],
      "title": "Error Rate",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
